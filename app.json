{
  "expo": {
    "name": "Kana master",
    "slug": "kana-master",
    "privacy": "public",
    "version": "1.1.0",
    "orientation": "portrait",
    "githubUrl": "https://github.com/nikkhvat/kana-master",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "pro.khvat.kanamaster",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./assets/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#1F1F1F"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
