{
  "expo": {
    "name": "Kana master",
    "slug": "kana-master",
    "privacy": "public",
    "version": "2.0.1",
    "orientation": "portrait",
    "githubUrl": "https://github.com/nikkhvat/kana-master",
    "icon": "./src/shared/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "pro.khvat.kanamaster",
      "splash": {
        "image": "./src/shared/assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./src/shared/assets/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#1F1F1F"
        }
      }
    },
    "android": {
      "icon": "./src/shared/assets/android-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/adaptive-icon.png",
        "backgroundColor": "#1F1F1F"
      },
      "splash": {
        "image": "./src/shared/assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./src/shared/assets/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#1F1F1F"
        }
      },
      "package": "pro.khvat.kanamaster",
      "version": "2.0.1",
      "versionCode": 3
    },
    "web": {
      "favicon": "./src/shared/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0c96ff0c-f16f-48ba-ac2f-b768df7666c3"
      }
    },
    "owner": "nikkhvat",
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
