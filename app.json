      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#FFFFFF",
        "dark": {
          "image": "./assets/splash-dark.png",
          "resizeMode": "cover",
          "backgroundColor": "#1F1F1F"
        }
      },
      "package": "pro.khvat.kanamaster",
      "version": "1.0.6",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0c96ff0c-f16f-48ba-ac2f-b768df7666c3"
      }
    },
    "owner": "nikkhvat",
    "plugins": [
      "expo-router"
